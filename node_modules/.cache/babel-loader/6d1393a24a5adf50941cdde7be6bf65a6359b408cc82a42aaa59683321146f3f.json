{"ast":null,"code":"import CochesData from \"@/components/vehiculos/CochesData\";\nimport MotosData from \"@/components/vehiculos/MotosData\";\nexport default {\n  components: {\n    CochesData: CochesData,\n    MotosData: MotosData\n  },\n  data: function data() {\n    return {\n      numeroCoches: null,\n      numeroMotos: null,\n      itemList: [{\n        marca: \"Suzuki\",\n        importeMoto: 10,\n        importeCoche: 20\n      }, {\n        marca: \"Honda\",\n        importeMoto: 100,\n        importeCoche: 200\n      }, {\n        marca: \"BMW\",\n        importeMoto: 1000,\n        importeCoche: 2000\n      }],\n      selectedItem: null\n    };\n  },\n  computed: {\n    precioCoches: function precioCoches() {\n      return this.numeroCoches * this.selectedItem.importeCoche;\n    },\n    precioMotos: function precioMotos() {\n      return this.numeroMotos * this.selectedItem.importeMoto;\n    },\n    precioTotal: function precioTotal() {\n      return this.precioCoches + this.precioMotos;\n    }\n  },\n  methods: {\n    updateNumeroCoches: function updateNumeroCoches(newValue) {\n      this.numeroCoches = newValue;\n    },\n    updateNumeroMotos: function updateNumeroMotos(newValue) {\n      this.numeroMotos = newValue;\n    }\n  }\n};","map":{"version":3,"names":["CochesData","MotosData","components","data","numeroCoches","numeroMotos","itemList","marca","importeMoto","importeCoche","selectedItem","computed","precioCoches","precioMotos","precioTotal","methods","updateNumeroCoches","newValue","updateNumeroMotos"],"sources":["src/components/vehiculos/VehiculosData.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    Número de vehiculos {{ numeroCoches + numeroMotos }} <br />\r\n    <br />\r\n    <br />\r\n    <button @click=\"$router.push({ name: 'management' })\">Ir a management</button>\r\n    <br />\r\n    <br />\r\n    <select v-model=\"selectedItem\">\r\n      <option v-for=\"item in itemList\" v-bind:key=\"item.marca\" :value=\"item\">{{ item.marca + ' C-' + item.importeCoche + '€, M-' + item.importeMoto + '€' }}</option>\r\n    </select>\r\n    <div v-if=\"selectedItem\">\r\n      <h3>Marca seleccionada:</h3>\r\n      <p>Nombre: {{ selectedItem.marca }}</p>\r\n      <p>Importe unidad Coche: {{ selectedItem.importeCoche }}€</p>\r\n      <p>Importe unidad Moto: {{ selectedItem.importeMoto }}€</p>\r\n      <br />\r\n      <br />\r\n      Precio total Coches: {{ numeroCoches }}uds x  {{ selectedItem.importeCoche }}€ = {{ precioCoches }}€ <br />\r\n      Precio total Motos: {{ numeroMotos }}uds x  {{ selectedItem.importeMoto }}€ = {{ precioMotos }}€ <br />\r\n      Precio Total: {{ precioTotal }}€ <br />\r\n      <br />\r\n      <br />\r\n      <CochesData @numeroCochesEvento=\"updateNumeroCoches\" />\r\n      <br />\r\n      <br />\r\n      <MotosData @numeroMotosEvento=\"updateNumeroMotos\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CochesData from \"@/components/vehiculos/CochesData\";\r\nimport MotosData from \"@/components/vehiculos/MotosData\";\r\n\r\nexport default {\r\n  components: {\r\n    CochesData,\r\n    MotosData,\r\n  },\r\n  data() {\r\n    return {\r\n      numeroCoches: null,\r\n      numeroMotos: null,\r\n      itemList: [\r\n        { marca: \"Suzuki\", importeMoto: 10, importeCoche: 20 },\r\n        { marca: \"Honda\", importeMoto: 100, importeCoche: 200 },\r\n        { marca: \"BMW\", importeMoto: 1000, importeCoche: 2000  },\r\n      ],\r\n      selectedItem: null,\r\n    };\r\n  },\r\n  computed: {\r\n    precioCoches() {\r\n      return this.numeroCoches * this.selectedItem.importeCoche;\r\n    },\r\n    precioMotos() {\r\n      return this.numeroMotos * this.selectedItem.importeMoto;\r\n    },\r\n    precioTotal() {\r\n      return this.precioCoches + this.precioMotos;\r\n    },\r\n  },\r\n  methods: {\r\n    updateNumeroCoches(newValue) {\r\n      this.numeroCoches = newValue;\r\n    },\r\n    updateNumeroMotos(newValue) {\r\n      this.numeroMotos = newValue;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"],"mappings":"AAgCA,OAAAA,UAAA;AACA,OAAAC,SAAA;AAEA;EACAC,UAAA;IACAF,UAAA,EAAAA,UAAA;IACAC,SAAA,EAAAA;EACA;EACAE,IAAA,WAAAA,KAAA;IACA;MACAC,YAAA;MACAC,WAAA;MACAC,QAAA,GACA;QAAAC,KAAA;QAAAC,WAAA;QAAAC,YAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,WAAA;QAAAC,YAAA;MAAA,GACA;QAAAF,KAAA;QAAAC,WAAA;QAAAC,YAAA;MAAA,EACA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACAC,YAAA,WAAAA,aAAA;MACA,YAAAR,YAAA,QAAAM,YAAA,CAAAD,YAAA;IACA;IACAI,WAAA,WAAAA,YAAA;MACA,YAAAR,WAAA,QAAAK,YAAA,CAAAF,WAAA;IACA;IACAM,WAAA,WAAAA,YAAA;MACA,YAAAF,YAAA,QAAAC,WAAA;IACA;EACA;EACAE,OAAA;IACAC,kBAAA,WAAAA,mBAAAC,QAAA;MACA,KAAAb,YAAA,GAAAa,QAAA;IACA;IACAC,iBAAA,WAAAA,kBAAAD,QAAA;MACA,KAAAZ,WAAA,GAAAY,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}